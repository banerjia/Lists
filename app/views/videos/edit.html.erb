<h2 id="create_a_new_entry">Create A New Entry</h2>

<%= form_for @video do |f| -%>
<%= render :partial => "video_entry", :locals => {:ff => f}%>
<div class="form_buttons">
	<%= f.submit "Update"%>
	<input type="button" value="Cancel" onclick="window.location='<%=videos_path%>'"/>		
</div>
<% end -%>
